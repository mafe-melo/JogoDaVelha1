        <Image x:Name="img1x2" HorizontalAlignment="Center" Height="100" Margin="0,69,0,0" VerticalAlignment="Top" Source="/Jogo da Velha 1.png" RenderTransformOrigin="0.844,0.203" Width="90" MouseLeftButtonDown="Click1x2" Visibility="Hidden"/>
        <Image x:Name="img2x2XBranco" HorizontalAlignment="Center" Height="87" Margin="0,177,0,0" VerticalAlignment="Top" Source="/Jogo da Velha 4.png" RenderTransformOrigin="0.844,0.203" Width="90" MouseLeftButtonDown="Click1x1"/>

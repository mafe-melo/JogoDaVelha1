        <TextBlock x:Name="txtJogador" HorizontalAlignment="Left" Margin="22,456,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="22" TextAlignment="Center" Height="29"><Run Text="Jogador"/><LineBreak/></TextBlock>
        <TextBlock x:Name="txtVelha" HorizontalAlignment="Left" Margin="193,456,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="22" TextAlignment="Center" Height="29"><Run Text="Velha"/><LineBreak/></TextBlock>
        <Ellipse HorizontalAlignment="Left" Height="21" Margin="516,84,0,0" Stroke="#FF248CDC" VerticalAlignment="Top" Width="22"/>
